---
import { Image } from '@astrojs/image/components';
import { findImage } from '~/utils/images';

const image = await findImage('/images/logo/BoisDebout-Creme.png');
---

<Image
  src={image}
  class="logo block h-10 lg:h-14 w-auto transition ease-in duration-250"
  alt={'Bois Debout Logo'}
  loading="lazy"
  decoding="async"
/>
